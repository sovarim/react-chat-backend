{
    "settings": {
        "import/resolver": {
          "node": {
            "paths": ["src"]
          }
        }
      },
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "rules": {
    }
}
